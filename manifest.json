{
  "manifest_version": 3,
  "name": "Skip Netflix Intros",
  "version": "1.0",
  "description": "Automatically clicks a button on a webpage",
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./scripts/content-script.js"]
    }
  ],
  "icons": {
    "16": "./icons/icon1.png",
    "48": "./icons/icon1.png",
    "128": "./icons/icon1.png"
  }
}
